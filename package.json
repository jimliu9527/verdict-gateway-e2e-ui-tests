{
    "name": "selenium-tests",
    "version": "1.0.0",
    "description": "Selenium tests for the unified client project",
    "main": "index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "interactive": "npx ts-node interactiveDriver.ts",
        "interactive:remote": "npx ts-node interactiveDriver.ts chrome http://10.61.133.195:4444",
        "test": "mocha -r ts-node/register --timeout 30000 '**/tests/*.spec.ts'",
        "getTheThings": "mocha -r ts-node/register --timeout 300000 '**/tests/getTheThings.spec.ts'",
        "expressionMatch": "mocha -r ts-node/register --timeout 300000 '**/tests/expressionMatch.spec.ts'",
        "schoolManagerPolicy": "mocha -r ts-node/register --timeout 300000 '**/tests/matchedSchoolManagerPolicy.spec.ts'",
        "regexMatcher": "mocha -r ts-node/register --timeout 300000 '**/tests/regexMatcher.spec.ts'",
        "testFile": "mocha -r ts-node/register --timeout 300000 pasture/testFile.spec.ts",
        "runZephyrTests": "mocha -r ts-node/register --timeout 1200000 tests/runZephyrTests.spec.ts"
    },
    "author": "David Ball, Kai Heath",
    "license": "ISC",
    "dependencies": {
        "@azure/storage-blob": "^12.24.0",
        "@types/chai": "^4.3.17",
        "archiver": "^7.0.1",
        "chai": "^5.1.1",
        "cross-env": "^7.0.3",
        "form-data": "^4.0.0",
        "mocha": "^10.7.0",
        "mocha-junit-reporter": "^2.2.1",
        "mochawesome": "^7.1.3",
        "selenium-webdriver": "^4.23.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "uuid": "^10.0.0",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@types/archiver": "^6.0.2",
        "@types/crypto-js": "^4.2.2",
        "@types/mocha": "^10.0.7",
        "@types/selenium-webdriver": "^4.1.25",
        "@types/uuid": "^10.0.0",
        "@types/xml2js": "^0.4.14",
        "crypto-js": "^4.2.0"
    }
}
